<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.textBox {
			color: rgba(0, 0, 0, 0.85);
		}
	</ui:style>
	<g:HTMLPanel ui:field='txtBoxWrapper' addStyleNames="input-field col s12">
       	<g:HTMLPanel ui:field="iconPanel" />
       	<g:TextBox ui:field="txtBox" addStyleNames="validate {style.textBox}"/>
      	 	<label for="first_name"><g:Label ui:field="lblName"/></label>
	</g:HTMLPanel>
</ui:UiBinder> 